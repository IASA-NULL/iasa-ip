{
  "name": "iasa-ip",
  "version": "4.0.6",
  "description": "IP 4.0.6",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IASA-Null/iasa-ip.git"
  },
  "author": "NULL <04seohyun@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IASA-Null/iasa-ip/issues"
  },
  "build": {
    "productName": "IP",
    "appId": "iasa.null.ip",
    "win": {
      "requestedExecutionLevel": "requireAdministrator",
      "extraFiles": [
        "./regStartup.bat",
        "./res/IPLogo.ico",
        "./res/iasa-ip-l.xml"
      ],
      "target": [
        {
          "target": "NSIS",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "./res/IPLogo.ico"
    },
    "nsis": {
      "createStartMenuShortcut": true,
      "perMachine": true
    }
  },
  "homepage": "https://github.com/IASA-Null/iasa-ip#readme",
  "dependencies": {
    "electron-settings": "^3.2.0",
    "network": "^0.4.1",
    "network-config": "^2.0.0",
    "request": "^2.88.0",
    "wifi-name": "^3.1.1"
  },
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "^21.2.0"
  }
}
