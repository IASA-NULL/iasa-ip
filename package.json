{
  "name": "iasa-ip",
  "version": "4.0.7",
  "description": "IP 4.0.7",
  "main": "index.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IASA-Null/iasa-ip.git"
  },
  "author": "NULL <ip@iasa.kr>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IASA-Null/iasa-ip/issues"
  },
  "build": {
    "productName": "IP",
    "appId": "iasa.null.ip",
    "directories": {
      "output": "build"
    },
    "win": {
      "requestedExecutionLevel": "requireAdministrator",
      "extraFiles": [
        "./res/IPLogo.ico",
        "./res/iasa-ip-l.xml"
      ],
      "target": [
        {
          "target": "NSIS",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "./res/IPLogo.ico"
    },
    "nsis": {
      "createStartMenuShortcut": true,
      "perMachine": true,
      "language": "65001"
    }
  },
  "homepage": "https://github.com/IASA-Null/iasa-ip#readme",
  "dependencies": {
    "electron-settings": "^3.2.0",
    "network": "^0.4.1",
    "network-config": "^2.0.0",
    "request": "^2.88.0",
    "wifi-name": "^3.1.1"
  },
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "^22.4.1"
  }
}
