{
  "name": "iasa-ip",
  "version": "4.1.0",
  "description": "IP 4.1.0",
  "main": "index.js",
  "scripts": {
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IASA-Null/iasa-ip.git"
  },
  "author": "NULL <ip@iasa.kr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IASA-Null/iasa-ip/issues"
  },
  "build": {
    "productName": "IP",
    "appId": "iasa.null.ip",
    "artifactName": "IP.${ext}",
    "directories": {
      "output": "build"
    },
    "win": {
      "requestedExecutionLevel": "requireAdministrator",
      "extraFiles": [
        "./res/IPLogo.ico",
        "./res/iasa-ip-l.xml"
      ],
      "target": [
        {
          "target": "NSIS",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "./res/IPLogo.ico"
    },
    "nsis": {
      "createStartMenuShortcut": true,
      "perMachine": true
    }
  },
  "homepage": "https://github.com/IASA-Null/iasa-ip#readme",
  "dependencies": {
    "electron-settings": "3.2.0",
    "request": "2.88.2",
    "wifi-name": "3.1.1"
  },
  "devDependencies": {
    "electron": "8.2.0",
    "electron-builder": "22.4.1"
  }
}
